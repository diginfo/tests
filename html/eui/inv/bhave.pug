#bhtabs.easyui-tabs(style='min-width:800px;' fit="true" tabPosition="left")        

  div(title='Service Items' iconCls="icon-repair")
    form#cprop.single.load(_sqlid='admin^bhave' asdpx='s')
      input(type="hidden" name="appid" value="inv^cp_man")
      +fitem('Menu Name',{name:'menu_inv^cp_man', class:'easyui-textbox'})
      +fitem('Dispositions',{name:'ACTION', class:'edit',id:'~'}) 
      //+fitem('UDF Layout',{'data-options':"data:dwap.pdata.udfid", id:'UDF_LAYOUT_ID',class:'easyui-combobox',name:'UDF_LAYOUT_ID'})
      //+fitem('UDF Layout',{'data-options':"url:'/',queryParams:{_sqlid:'vwltsa^udf_layout',_func:'get'}", id:'UDF_LAYOUT_ID',class:'easyui-combobox',name:'UDF_LAYOUT_ID'})
      +fitem('UDF Layout',{'_sqlid':"vwltsa^udfid", id:'UDF_LAYOUT_ID',class:'easyui-combobox',name:'UDF_LAYOUT_ID'})
  div(title='Parts' iconCls="icon-part")
    form.single.load(_sqlid='admin^bhave' asdpx='s')
      +input({name:'appid', type:'hidden',value:'inv^sa_parts'})
      br
      +fieldset('Mandatory fields')
        +fitem('Revision',{name:'MANDATORY_REVISION', class:'easyui-combobox','data-options':"data:[{text:'YES',value:'y'},{text:'NO',value:'n',selected:true}]" })
      +fieldset('Non-Tracable')
        +fitem('Mandatory - Unit Cost ',{name:'MANDATORY_UNITCOST', class:'easyui-combobox','data-options':"data:[{text:'YES',value:'y'},{text:'NO',value:'n',selected:true}]" })
        
      +fieldset('User Entered')
        +fitem('Traceable',{name:'partTraceManual', class:'easyui-combobox','data-options':"data:[{text:'YES',value:'y'},{text:'NO',value:'n',selected:true}]" })
      +fieldset('Features')
        +fitem('Enable Generator',{name:'partgen', class:'easyui-combobox','data-options':"data:[{text:'YES',value:'y'},{text:'NO',value:'n',selected:true}]" })

      +fieldset('Generator ID')
        +fitem('Null ID',{name:'idnull', class:'easyui-textbox'})
                
      +fieldset('Generator Description')
        +fitem('Null ID',{name:'descnull', class:'easyui-textbox'})
        +fitem('Prefix-Suffix',{name:'prefixSuffix', class:'easyui-textbox'})
        +fitem('Suffix-Prefix',{name:'suffixPrefix', class:'easyui-textbox'})
        +fitem('Separator Padding',{name:'padsep', class:'easyui-combobox','data-options':"data:[{text:'NONE',value:'n',selected:true},{text:'LEFT',value:'l'},{text:'RIGHT',value:'r'},{text:'BOTH',value:'b'}]" })
          
  div(title='Part Trans' iconCls="icon-inout")
    form#parttrans.single.load(_sqlid='admin^bhave' asdpx='s')
      input(type="hidden" name="appid" value="inv^part_trans")
      +fitem('FG Max Receipt',{name:'FG_MAXRECV', class:'easyui-numberspinner' ,value:100, min:100, precision:0, suffix:'%'})
      +fitem('Allowed Misc Tx',{name:'ALLOWED_MISC_TX', class:'easyui-combobox','data-options':"data:[{text:'YES',value:'y'},{text:'NO',value:'n',selected:true}]" })
      +fitem('Allowed Edit Date',{name:'ALLOWED_EDIT_TXDATE', class:'easyui-combobox','data-options':"data:[{text:'YES',value:'y'},{text:'NO',value:'n',selected:true}]" })
      br
      +fieldset('FG Receipt') 
        +fitem('Use Job as Trace ID',{name:'FG_IN_DEF_JOB_AS_TRACE', class:'easyui-combobox','data-options':"data:[{text:'YES',value:'y'},{text:'NO',value:'n',selected:true}]" })
        +fitem('Unit Trace Cost',{name:'FG_IN_UNIT_TRACE_COST', class:'easyui-combobox','data-options':"data:[{text:'Part Master',value:'P'},{text:'Job',value:'J',selected:true}]" })
        +fitem('Trace Cost Overwrite',{name:'FG_IN_TRACE_COST_OVERWRITE', class:'easyui-combobox','data-options':"data:[{text:'YES',value:'y'},{text:'NO',value:'n',selected:true}]" })

      +fieldset('Expired Materials')
        +fitem('Allow Job Issue',{name:'EXPIRED_ALLOW_JOB_OUT', class:'easyui-combobox','data-options':"data:[{text:'YES',value:'y'},{text:'NO',value:'n',selected:true}]" })
      +fieldset('WIP Count')
        +fitem('Allow Job Issue',{name:'WIP_COUNT_ALLOW_JOB_OUT', class:'easyui-combobox','data-options':"data:[{text:'YES',value:'y'},{text:'NO',value:'n',selected:true}]" })
script(type="text/javascript"): include js/bhave.js
